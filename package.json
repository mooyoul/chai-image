{
  "name": "chai-image",
  "version": "3.0.0",
  "description": "Extends Chai with assertions about images",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "index.js",
    "global.d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "mocha -r ts-node/register index.spec.ts",
    "lint": "tslint -c tslint.json index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mooyoul/chai-image.git"
  },
  "keywords": [
    "chai",
    "image",
    "pixeldiff",
    "chai-plugin",
    "imagediff"
  ],
  "author": "MooYeol Prescott Lee <mooyoul@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mooyoul/chai-image/issues"
  },
  "homepage": "https://github.com/mooyoul/chai-image#readme",
  "devDependencies": {
    "@prescott/commitlint-preset": "1.0.3",
    "@prescott/semantic-release-config": "1.0.1",
    "@prescott/tslint-preset": "1.0.1",
    "@types/chai": "4.3.3",
    "@types/mkdirp": "1.0.2",
    "@types/mocha": "9.1.1",
    "@types/node": "16.11.58",
    "@types/pixelmatch": "5.2.4",
    "@types/pngjs": "6.0.1",
    "chai": "4.3.6",
    "del": "6.1.1",
    "husky": "8.0.1",
    "mocha": "10.0.0",
    "ts-node": "10.9.1",
    "tslint": "6.1.3",
    "typescript": "4.8.2"
  },
  "dependencies": {
    "mkdirp": "^1.0.0",
    "pixelmatch": "^5.0.2",
    "pngjs": "^6.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@prescott/commitlint-preset"
    ]
  }
}
